<h1>Perfil de usuario</h1>
<p>
  <a href="/products"><button type="button">Ir a productos</button></a>
  <button type="button" id="btnLogout">Cerrar sesión</button>
</p>
<p>
  <img src="/profiles/_blank.jpg" alt="profile image" id="imgProfile" height="150px" />
</p>
<form id="frmUpdate" enctype="multipart/form-data">
  <labe>Nombre: </labe>
  <input type="text" id="first_name" name="first_name" disabled />
  <br />
  <br />
  <labe>Apellido: </labe>
  <input type="text" id="last_name" name="last_name" disabled />
  <br />
  <br />
  <labe>Edad: </labe>
  <input type="number" id="age" name="age" disabled />
  <br />
  <br />
  <labe>Email: </labe>
  <input type="email" id="email" name="email" disabled />
  <br />
  <br />
  <labe>Imagen de perfil: </labe>
  <input type="file" id="profile" name="profile" accept="image/*" />
  <br />
  <br />
  <labe>Comprobante de identidad: </labe>
  <input type="file" id="identity" name="identity" accept="image/*,.pdf" />
  <br />
  <br />
  <labe>Comprobante de domicilio: </labe>
  <input type="file" id="residence" name="residence" accept="image/*,.pdf" />
  <br />
  <br />
  <labe>Comprobante de estado de cuenta: </labe>
  <input type="file" id="account" name="account" accept="image/*,.pdf" />
  <br />
  <br />
  <labe>Imágenes de productos: </labe>
  <input type="file" id="product" name="product" accept="image/*" multiple />
  <br />
  <br />
  <button type="button" id="btnUpdate">Actualizar perfil</button>
</form>

<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
<script src="/js/user-profile.js"></script>