<div>
  <h1>PRODUCTOS</h1>
  <h2>Bienvenido {{user.first_name}} {{user.last_name}} ({{user.role}})</h2>
  <ul>
    {{#each productsList.payload}}
      <li>{{this.id}}
        -
        {{this.title}}
        ({{this.code}}) |
        <button>Ver detalle</button>
        |
        <button>Agregar al carrito</button>
        |</li>
    {{/each}}
  </ul>
  <br />
  <span>PÃ¡gina {{productsList.page}}/{{productsList.totalPages}}</span>
  {{#if productsList.hasPrevPage}}
    <button><a href={{productsList.prevLink}}>Anterior</a></button>
  {{/if}}
  {{#if productsList.hasNextPage}}
    <button><a href={{productsList.nextLink}}>Siguiente</a></button>
  {{/if}}
  <p><button id="btnLogout">Salir</button></p>
</div>